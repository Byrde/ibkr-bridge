{
  "openapi": "3.0.3",
  "info": {
    "title": "IBKR Gateway API",
    "description": "Minimal API for IBKR Client Portal Gateway authentication and request proxying",
    "version": "0.1.0"
  },
  "components": {
    "securitySchemes": {
      "basicAuth": {
        "type": "http",
        "scheme": "basic",
        "description": "IBKR credentials (username:password)"
      }
    },
    "schemas": {}
  },
  "paths": {
    "/api/v1/health": {
      "get": {
        "tags": [
          "Health"
        ],
        "description": "Gateway health check",
        "responses": {
          "200": {
            "description": "Default Response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "enum": [
                        "healthy",
                        "degraded"
                      ],
                      "example": "healthy"
                    },
                    "gateway": {
                      "type": "object",
                      "properties": {
                        "status": {
                          "type": "string",
                          "example": "running"
                        },
                        "healthy": {
                          "type": "boolean",
                          "example": true
                        },
                        "pid": {
                          "type": "number",
                          "example": 12345
                        },
                        "restartCount": {
                          "type": "number",
                          "example": 0
                        }
                      }
                    },
                    "timestamp": {
                      "type": "string",
                      "format": "date-time"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/auth/login": {
      "post": {
        "tags": [
          "Auth"
        ],
        "description": "Authenticate with IBKR Gateway using headless browser login.\n\nIBKR credentials are passed via Basic Auth. Use the **Authorize** button to set credentials.\n\n**Headers:**\n- `Authorization`: Basic Auth with IBKR username:password (required)\n- `X-TOTP-Secret`: TOTP secret for 2FA, base32 encoded (required for live trading)\n- `X-Paper-Trading`: Set to \"true\" for paper trading mode (optional)",
        "parameters": [
          {
            "schema": {
              "type": "string"
            },
            "in": "header",
            "name": "x-totp-secret",
            "required": false,
            "description": "TOTP secret for 2FA (base32 encoded). Required for live trading."
          },
          {
            "schema": {
              "type": "string",
              "enum": [
                "true",
                "false"
              ]
            },
            "in": "header",
            "name": "x-paper-trading",
            "required": false,
            "description": "Enable paper trading mode. No TOTP required for paper trading."
          }
        ],
        "security": [
          {
            "basicAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Default Response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean",
                      "example": true
                    },
                    "message": {
                      "type": "string",
                      "example": "Authentication successful"
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Default Response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean",
                      "example": false
                    },
                    "error": {
                      "type": "string",
                      "example": "Authentication failed"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/v1/api/{*}": {
      "get": {
        "tags": [
          "Proxy"
        ],
        "description": "All requests under /v1/api/* are proxied directly to the IBKR Client Portal Gateway. Refer to IBKR documentation for available endpoints.",
